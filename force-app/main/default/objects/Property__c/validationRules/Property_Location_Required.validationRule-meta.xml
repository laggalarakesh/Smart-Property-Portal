<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Property_Location_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISBLANK(Location__c),
    NOT($Setup.Test_Config__c.Bypass_Automations__c)
)</errorConditionFormula>
    <errorMessage>Location is required for all properties</errorMessage>
</ValidationRule>
